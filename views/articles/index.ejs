<!DOCTYPE html>
<html>
<head>
  <title>Articles</title>
  <link rel="icon" href="/favicon.ico">
  <link rel="stylesheet" href="/styles.css">
</head>

<body class="<%= theme %>">
  <h1>Articles</h1>
  <ul>
    <% articles.forEach(article => { %>
      <li><a href="/articles/<%= article.id %>"><%= article.title %></a></li>
    <% }) %>
  </ul>
</body>
</html>
